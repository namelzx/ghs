/* 弹幕样式 */
.barrage-content{
  position: fixed;
  top: 100rpx;
  left: 60rpx;
  display: flex;
  align-items: center;
  width: 300rpx;
  height: 46rpx;
  background-color: rgba(251, 94, 81, 0.7);
  border-radius: 23rpx;
  z-index: 10;
  opacity: 1;
  animation: danmu 5s linear infinite;
}
@keyframes danmu{
  0%   {opacity: 0;}
  20%,60%{opacity: 1;}
  70% {opacity: 0.8;}
  75% {opacity: 0.6;}
  80%,100% {opacity: 0;}
}
.barrage-content image{
  margin-left: 4rpx;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
}
.barrage-text{
  flex: 1;
  width: 1;
  overflow: hidden;
  text-overflow: ellipsis;
