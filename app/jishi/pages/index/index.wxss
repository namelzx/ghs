/* 入驻指南 */
.shopper-in-content{
  width: 690rpx;
  height: 287rpx;
  margin: 60rpx auto 0;
  padding: 30rpx 42rpx 0;
  box-sizing: border-box;
  border: 2rpx solid #FFE200;
  border-radius: 16rpx;
  position: relative;
}
.shopper-in-content.fixed{
  position: fixed;
  bottom: 30rpx;
  left: 50%;
  transform: translateX(-50%);
}
.shopper-in-top{
  position: absolute;
  left: 42rpx;
  top: -26rpx;
  display: flex;
  width: 130rpx;
  height: 52rpx;
}
.shopper-in-top image{
  width: 100%;
  height: 100%;
}
.shopper-in-item{
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #f7f7f7;
  padding: 23rpx 0 23rpx 20rpx;
}
.shopper-in-item:last-child{
  border: none;
}
.shopper-in-item-title{
  font-family: PingFangSC-Medium;
  font-size: 26rpx;
  color: #333333;
  line-height: 46rpx;
}
.shopper-in-item-phone-box{
