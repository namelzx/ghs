/* page/tabBar/index/index.wxss */
.P-index{
  min-height: 100vh;
  background-color: #fff;
}
.P-index.limitHeight{
  height: 100vh;
  overflow: hidden;
}
/* 头部样式 */
.header-container{
  background-color: #fff;
  display: flex;
  /* align-items: center;
  justify-content: space-between; */
  /* height: 110rpx; */
  padding: 0 25rpx;
  font-family: PingFangSC-Medium;
  height: 332rpx;
  position: relative;
}
.header-bg image{
  position: absolute;
  left: 0;
  right: 0;
  height: 290rpx;
  z-index: 1;
}
.header-content{
  position: absolute;
  top: 0;
  left: 30rpx;
  width: 700rpx;
  height: 92rpx;
  /* flex: 1; */
  z-index: 2;
  display: flex;
  align-items: center;
  justify-content: space-between;
  overflow: hidden;
}
.header-content-left{
  flex: 1;
  width: 1;
  display: flex;
  align-items: center;
  margin-right: 30rpx;
  overflow: hidden;
}

.location-content{
  display: flex;
  align-items: center;
  font-size: 0rpx;
  /* flex: 1; */
  overflow: hidden;
}
.location-content image{
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  margin-right: 12rpx;
}
.location-content view{
  font-size: 28rpx;
  color: #000000;
  flex: 1;
  width: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.icon-arrow-bottom{
  margin-left: 8rpx;
  width: 16rpx;
  height: 8rpx;
}
.chat-btn{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #333333;
}
.chat-btn image{
  width: 44rpx;
  height: 44rpx;
  margin-right: 4rpx
}
.btn-search{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 225rpx;
  height: 55rpx;
  background: #F5F5F5;
  border-radius: 30rpx;
}
.btn-search image{
  width: 31rpx;
  height: 31rpx;
  margin-right: 4rpx;
}
.btn-search-text{
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #999999;
}

/* 轮播图样式 */
.swiper-container{
  position: absolute;
  top: 92rpx;
  z-index: 3;
  padding: 0 25rpx;  
  /* width: 750rpx; */
  width: 100%;
  box-sizing: border-box;
  height: 240rpx;
}

/* 产品分类导航栏 */
.scroll-bar-view{
  background-color: #fff;
  position: absolute;
  /* top: 350rpx;  */
  /* 这里不能设置太距离顶部，否则下拉会出现一点 */
  left: 0;
  right: 0;
  z-index: 10;
  height: 94rpx;
  /* transition: top 200ms; */
}
.scroll-bar-view.active{
  position: fixed;
  top: 0rpx;
  /* transition: top 200ms; */
}

.filter-bar-container{
  white-space: nowrap;
  height: 94rpx;
  line-height: 94rpx;
  padding: 0 45rpx 0 25rpx;
  box-sizing: border-box;
  background-color: #fff;
  /* box-shadow: 0 10rpx 16rpx 0 rgba(183,181,181,0.12); */
}
.filter-bar-container.fixed{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 10;
}
.filter-item{
  display: inline-block;
  min-width: 112rpx;
  /* text-align: center; */
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #282828;
  padding: 12rpx 0rpx;
  box-sizing: border-box;
  margin-left: 22rpx;
  margin-right: 22rpx;
  line-height: 28rpx;
  position: relative;
  height: 94rpx;
}
.filter-item.active{
  /* background: #FFE849; */
  font-size: 30rpx;
  border-radius: 24rpx;
  color: #333;
  font-family: PingFangSC-Semibold;
}
.filter-item.short{
  min-width: 56rpx;
}
.filter-item-text{
  z-index: 2;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.filter-item-bottom{
  position: absolute;
  /* bottom: -8rpx; */
  left: 0;
  right: 0;
  top:45rpx;
  height: 20rpx;
  background-color: #FFE849;
  z-index: 1;
}

