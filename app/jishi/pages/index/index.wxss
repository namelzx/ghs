/* 轮播图样式 */
.swiper-container{
  position: absolute;
  top: 92rpx;
  z-index: 3;
  padding: 0 25rpx;  
  /* width: 750rpx; */
  width: 100%;
  box-sizing: border-box;
  height: 240rpx;
}

/* 产品分类导航栏 */
.scroll-bar-view{
  background-color: #fff;
  position: absolute;
  /* top: 350rpx;  */
  /* 这里不能设置太距离顶部，否则下拉会出现一点 */
  left: 0;
  right: 0;
  z-index: 10;
  height: 94rpx;
  /* transition: top 200ms; */
}
.scroll-bar-view.active{
  position: fixed;
  top: 0rpx;
  /* transition: top 200ms; */
}

.filter-bar-container{
  white-space: nowrap;
  height: 94rpx;
  line-height: 94rpx;
  padding: 0 45rpx 0 25rpx;
  box-sizing: border-box;
  background-color: #fff;
  /* box-shadow: 0 10rpx 16rpx 0 rgba(183,181,181,0.12); */
}
.filter-bar-container.fixed{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 10;
}
.filter-item{
  display: inline-block;
  min-width: 112rpx;
  /* text-align: center; */
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #282828;
  padding: 12rpx 0rpx;
  box-sizing: border-box;
  margin-left: 22rpx;
  margin-right: 22rpx;
  line-height: 28rpx;
  position: relative;
  height: 94rpx;
}
.filter-item.active{
  /* background: #FFE849; */
  font-size: 30rpx;
  border-radius: 24rpx;
  color: #333;
  font-family: PingFangSC-Semibold;
}
.filter-item.short{
  min-width: 56rpx;
}
.filter-item-text{
  z-index: 2;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.filter-item-bottom{
  position: absolute;
  /* bottom: -8rpx; */
  left: 0;
  right: 0;
  top:45rpx;
  height: 20rpx;
  background-color: #FFE849;
  z-index: 1;
}

