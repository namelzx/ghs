/* pages/supplier/protocol/datum/index.wxss */
.pro-img-list {
  padding: 20rpx 30rpx;
}
.list-title {
  margin-bottom: 24rpx;
  font-size: 28rpx;
  color: #323232;
}

.post-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: #fff;
  padding: 28rpx 0;
  z-index: 9;
  box-shadow: 0 0 10rpx 0 #d5d5d5;
}

.post-btn {
  width: 30%;
  margin-left: 35%;
  background: #fdaa5a;
  text-align: center;
  padding: 8px 0;
  border-radius: 10rpx;
  color: #fff;
  font-size: 28rpx;
}