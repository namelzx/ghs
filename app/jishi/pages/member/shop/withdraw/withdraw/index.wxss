/* pages/invite/withdraw/withdraw.wxss */


.item{
    width: 100%;
    box-sizing: border-box;
    padding: 14rpx 20rpx;
    border-bottom: 3rpx solid #eee;
    margin: 6rpx 0;
}
.item view{
   font-size: 32rpx;
   color:#333;
}

.item text{
   display: block;
   font-size: 32rpx;
   color:#333;
   margin-top: 14rpx;
}

.item input{
   display: block;
   width: 100%;
   font-size: 28rpx;
   margin-top:10rpx;
}

.input-placeholder{
   color:#c2c2c2;
}

.btn{
    width: 300rpx;
    margin-top: 80rpx;
    height:80rpx;
    /* line-height: 80rpx; */
    border-radius: 10rpx;
    text-align: center;
    color: #ffff;
    font-size: 30rpx;
    font-weight: 800;
    margin-left: 30%;
}
button{
  background: #333;
}
